<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <filter>
        <filter-name>meinvFilter</filter-name>
        <filter-class>com.atcdu.liujun.myFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>meinvFilter</filter-name>
        <url-pattern>/meinv.jsp</url-pattern>
    </filter-mapping>



    <servlet>
        <servlet-name>firstServlet</servlet-name>
        <servlet-class>com.atcdu.liujun.myServerlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>firstServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>



</web-app>
